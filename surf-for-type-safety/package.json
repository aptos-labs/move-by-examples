{
  "name": "aptos-full-stack-template",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "contracts/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "move:init": "npm run init -w @repo/message-board-contract",
    "move:test": "npm run test -w @repo/message-board-contract",
    "move:publish": "npm run publish -w @repo/message-board-contract",
    "move:upgrade": "npm run upgrade -w @repo/message-board-contract",
    "move:get-abis": "npm run get-abis -w @repo/move-contract-abis",
    "script:post-message": "npm run post-message -w @repo/node-script",
    "script:get-messages": "npm run get-messages -w @repo/node-script",
    "script:get-message-content": "npm run get-message-content -w @repo/node-script",
    "script:get-message-struct": "npm run get-message-struct -w @repo/node-script"
  },
  "devDependencies": {
    "@repo/eslint-config-adapter": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.6.3"
}
